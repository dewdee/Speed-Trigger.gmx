            <string>//counter increment
counter++;

//Movement
//Double jump shenanigans
if(place_meeting(x,y+1, obj_ground)){
    jump = jump_max
}
if((keyboard_check_pressed(ord('W')) || keyboard_check_pressed(vk_up)) &amp;&amp; (jump &gt; 0)){ //jump
    if(jump = 1){
        vspeed -= 4.0;
    }
    else{
        vspeed -= 7;
    }
    jump -= 1;
}
if(place_free(x + 5,y) &amp;&amp; (keyboard_check(ord('D')) || keyboard_check(vk_right))){
   if(!position_meeting(x + 60, y, obj_wallRight)){ //wall collision
        x += 5;
    }
}
if(place_free(x - 5,y) &amp;&amp; (keyboard_check(ord('A')) || keyboard_check(vk_left))){
    if(!position_meeting(x, y, obj_wallLeft)){ //wall collision
        x -= 5;
    }
}
if(keyboard_check_pressed(vk_space)){
    instance_create((x + sprite_width / 2), y, obj_laser);
}

//Gravity
if(place_free(x,y + 1)){
    gravity = 0.56;
}
else{
    gravity = 0;
}

//HP
if(hp &lt;= 0 &amp;&amp; !instance_exists(obj_gameOver)){
    object_set_persistent(obj_player, false);
    instance_destroy();
    highscore_add("Player 1", global.score);
    instance_create(room_width / 2, room_height, obj_gameOver);
    audio_play_sound(snd_crash, 10, false);
    audio_pause_sound(snd_BGM);
}

//Fell down
if(y &gt; room_height + 32 &amp;&amp; !instance_exists(obj_gameOver)){
    object_set_persistent(obj_player, false);
    instance_destroy();
    highscore_add("Player 1", global.score);
    instance_create(room_width / 2, room_height, obj_gameOver);
    audio_play_sound(snd_crash, 10, false);
    audio_pause_sound(snd_BGM);
}

//Advancing levels
if(global.score = 500){ //level 2
    room_goto(rm_level2);
}
if(global.score = 1500){ //level 3
    room_goto(rm_level3);
}
if(global.score = 3000){ //level 3
    room_goto(rm_level4);
}
if(global.score = 6000){ //level 3
    room_goto(rm_level5);
}
